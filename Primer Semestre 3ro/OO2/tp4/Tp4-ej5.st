Object subclass: #CalcularSueldos	instanceVariableNames: ''	classVariableNames: ''	package: 'Tp4-ej5'!!CalcularSueldos methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 21:58'!calcularSueldo	^ ((self basico + self adicional) - self descuento)! !!CalcularSueldos methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 21:24'!basico	^self subclassResponsibility ! !!CalcularSueldos methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 21:24'!adicional	^self subclassResponsibility ! !!CalcularSueldos methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 22:48'!descuento	^(((self basico) * 0.13) + ((self adicional) * 0.05))	! !CalcularSueldos subclass: #Pasante	instanceVariableNames: 'examenes'	classVariableNames: ''	package: 'Tp4-ej5'!!Pasante methodsFor: 'constructor' stamp: 'FerminMoreno 5/28/2020 21:51'!cantidadExamenes: cantExamenes	examenes:= cantExamenes 	! !!Pasante methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 21:51'!basico	^2000. ! !!Pasante methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 21:52'!adicional	^(100 * examenes)! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Pasante class	instanceVariableNames: ''!!Pasante class methodsFor: 'initialize' stamp: 'FerminMoreno 5/29/2020 19:05'!cantidadExamenes: cantExamenes	^self new cantidadExamenes: cantExamenes;yourself ! !CalcularSueldos subclass: #Planta	instanceVariableNames: 'casado hijos antiguedad'	classVariableNames: ''	package: 'Tp4-ej5'!!Planta methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 22:05'!basico	^3000! !!Planta methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 21:56'!adicional	(casado)	ifTrue: [ ^((100 * hijos)+ (100 * antiguedad) + (500) ) ]	ifFalse: [ ^((100 * hijos)+ (100 * antiguedad)) ]! !!Planta methodsFor: 'constructor' stamp: 'FerminMoreno 5/28/2020 21:52'!años: unaAntiguedad hijos: cantHijos casado: estaCasado	antiguedad:= unaAntiguedad.	hijos:= cantHijos.	casado:= estaCasado.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Planta class	instanceVariableNames: ''!!Planta class methodsFor: 'initialize' stamp: 'FerminMoreno 5/29/2020 19:05'!años: unaAntiguedad hijos: cantHijos casado: estaCasado	^self new años: unaAntiguedad hijos: cantHijos casado: estaCasado;yourself ! !CalcularSueldos subclass: #Temporario	instanceVariableNames: 'horasDeTrabajo casado hijos'	classVariableNames: ''	package: 'Tp4-ej5'!!Temporario methodsFor: 'constructor' stamp: 'FerminMoreno 5/28/2020 21:52'!horasDeTrabajo: hs hijos: cantHijos casado: estaCasado	hijos:= cantHijos.	casado:= estaCasado.	horasDeTrabajo:= hs.	! !!Temporario methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 22:07'!basico	^( (4000) + (horasDeTrabajo * 10) )! !!Temporario methodsFor: 'public' stamp: 'FerminMoreno 5/28/2020 22:06'!adicional	(casado)	ifTrue: [ ^((100 * hijos) + 500) ]	ifFalse: [ ^(100 * hijos) ]! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Temporario class	instanceVariableNames: ''!!Temporario class methodsFor: 'initialize' stamp: 'FerminMoreno 5/29/2020 19:05'!horasDeTrabajo: hs hijos: cantHijos casado: estaCasado	^self new horasDeTrabajo: hs hijos: cantHijos casado: estaCasado;yourself ! !TestCase subclass: #TestCalcularSueldos	instanceVariableNames: 'temporario1 pasante1 planta1'	classVariableNames: ''	package: 'Tp4-ej5'!!TestCalcularSueldos methodsFor: 'setUp' stamp: 'FerminMoreno 5/28/2020 22:36'!setUp	pasante1 := Pasante cantidadExamenes: 0.	temporario1 := Temporario horasDeTrabajo: 100 hijos: 1 casado: true.	planta1:= Planta años: 1 hijos: 1 casado: true.	! !TestCalcularSueldos subclass: #TestPasante	instanceVariableNames: ''	classVariableNames: ''	package: 'Tp4-ej5'!!TestPasante methodsFor: 'test' stamp: 'FerminMoreno 5/28/2020 22:54'!testAdicional	self assert: (pasante1 adicional) equals: 0.	pasante1 := Pasante cantidadExamenes: 10.	self assert: (pasante1 adicional) equals: 1000.! !!TestPasante methodsFor: 'test' stamp: 'FerminMoreno 5/28/2020 22:53'!testBasico	self assert: (pasante1 basico) equals: 2000.! !TestCalcularSueldos subclass: #TestPlanta	instanceVariableNames: ''	classVariableNames: ''	package: 'Tp4-ej5'!!TestPlanta methodsFor: 'test' stamp: 'FerminMoreno 5/28/2020 22:59'!testAdicional	self assert: (planta1 adicional) equals: 700.	planta1:= Planta años: 1 hijos: 0 casado: true.	self assert: (planta1 adicional) equals: 600.	planta1:= Planta años: 1 hijos: 0 casado: false.	self assert: (planta1 adicional) equals: 100.	planta1:= Planta años: 0 hijos: 3 casado: true.	self assert: (planta1 adicional) equals: 800.	planta1:= Planta años: 1 hijos: 3 casado: false.	self assert: (planta1 adicional) equals: 400.! !!TestPlanta methodsFor: 'test' stamp: 'FerminMoreno 5/28/2020 22:56'!testBasico	self assert: (planta1 basico) equals: 3000.! !TestCalcularSueldos subclass: #TestTemporario	instanceVariableNames: ''	classVariableNames: ''	package: 'Tp4-ej5'!!TestTemporario methodsFor: 'test' stamp: 'FerminMoreno 5/28/2020 22:44'!testAdicional	self assert: (temporario1 adicional) equals: 600.	temporario1 := Temporario horasDeTrabajo: 0 hijos: 0 casado: true.	self assert: (temporario1 adicional) equals: 500.	temporario1 := Temporario horasDeTrabajo: 0 hijos: 2 casado: false.	self assert: (temporario1 adicional) equals: 200.! !!TestTemporario methodsFor: 'test' stamp: 'FerminMoreno 5/28/2020 22:50'!testDescuento	self assert: (temporario1 descuento) equals: 680.	temporario1 := Temporario horasDeTrabajo: 100 hijos: 0 casado: true.	self assert: (temporario1 descuento) equals: 675.	temporario1 := Temporario horasDeTrabajo: 100 hijos: 2 casado: false.	self assert: (temporario1 descuento) equals: 660.	temporario1 := Temporario horasDeTrabajo: 0 hijos: 2 casado: false.	self assert: (temporario1 descuento) equals: 530.	! !!TestTemporario methodsFor: 'test' stamp: 'FerminMoreno 5/28/2020 22:52'!testCalcularSueldo	self as