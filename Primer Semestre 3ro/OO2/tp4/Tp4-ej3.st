Object subclass: #Client	instanceVariableNames: 'topografia'	classVariableNames: ''	package: 'Tp4-ej3'!TestCase subclass: #TestMixta	instanceVariableNames: 'topUnica1 topUnica2 topMixta'	classVariableNames: ''	package: 'Tp4-ej3'!!TestMixta methodsFor: 'running' stamp: 'lautaro 5/27/2020 19:23'!setUp	topUnica1 := Agua new.	topUnica2 := Tierra new.	topMixta := Mixta new! !!TestMixta methodsFor: 'test' stamp: 'lautaro 5/27/2020 19:27'!testComprobarIgualdad	|topMixta1 topMixta2|	topMixta1 := Mixta new.	topMixta1 add: topUnica1.	topMixta1 add: topUnica2.	topMixta1 add: topUnica1.	topMixta1 add: topUnica2.	topMixta2 := Mixta new.	topMixta1 add: topUnica1.	topMixta2 add: topUnica2.	topMixta2 add: topUnica1.	topMixta2 add: topUnica2.	self assert: (topMixta1 comprobarIgualdad: topMixta2) equals: true.! !!TestMixta methodsFor: 'test' stamp: 'lautaro 5/27/2020 19:24'!testCalcularProporcion	|topMixta1 topMixta2 topMixta3 topMixta4| 	topMixta1 := Mixta new.	topMixta1 add: topUnica1.	topMixta1 add: topUnica2.	topMixta1 add: topUnica1.	topMixta1 add: topUnica2.	topMixta add: topMixta1.	self assert: (topMixta calcularProporcion) equals: 0.5.! !Object subclass: #Topografia	instanceVariableNames: ''	classVariableNames: ''	package: 'Tp4-ej3'!!Topografia methodsFor: 'public' stamp: 'lautaro 5/26/2020 21:19'!comprobarIgualdad: top1	^self subclassResponsibility.	! !!Topografia methodsFor: 'public' stamp: 'lautaro 5/26/2020 19:58'!calcularProporcion 	^self subclassResponsibility.! !!Topografia methodsFor: 'accessing' stamp: 'lautaro 5/27/2020 18:56'!topografias	^false.! !Topografia subclass: #Agua	instanceVariableNames: ''	classVariableNames: ''	package: 'Tp4-ej3'!!Agua methodsFor: 'public' stamp: 'lautaro 5/27/2020 19:25'!proporcion	^1.! !!Agua methodsFor: 'public' stamp: 'lautaro 5/27/2020 18:53'!comprobarIgualdad: top2	^(self calcularProporcion = top2 calcularProporcion).! !!Agua methodsFor: 'public' stamp: 'lautaro 5/27/2020 19:25'!calcularProporcion	^self proporcion.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Agua class	instanceVariableNames: ''!!Agua class methodsFor: 'initalize' stamp: 'lautaro 5/26/2020 20:48'!new: propor	^self new new: propor; yourself.! !Topografia subclass: #Mixta	instanceVariableNames: 'topografias'	classVariableNames: ''	package: 'Tp4-ej3'!!Mixta methodsFor: 'public' stamp: 'lautaro 5/26/2020 19:55'!add: topografia	topografias add: topografia.! !!Mixta methodsFor: 'public' stamp: 'lautaro 5/26/2020 21:20'!comprobarIgualdad: top2	|cal1 cal2|	cal1 := self calcularProporcion.	cal2 := top2 calcularProporcion.	^((cal1 = cal2) & (self topografias = top2 topografias))! !!Mixta methodsFor: 'public' stamp: 'lautaro 5/27/2020 19:11'!calcularProporcion 	^((self proporcion) / 4).	! !!Mixta methodsFor: 'public' stamp: 'lautaro 5/27/2020 19:26'!proporcion	^(topografias sumNumbers: [ :topo | topo proporcion ]).! !!Mixta methodsFor: 'public' stamp: 'lautaro 5/26/2020 19:56'!remove: topografia	topografias remove: topografia.! !!Mixta methodsFor: 'accessing' stamp: 'lautaro 5/26/2020 20:36'!topografias	^topografias.! !!Mixta methodsFor: 'initialize' stamp: 'lautaro 5/27/2020 19:02'!initialize	topografias := OrderedCollection new.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Mixta class	instanceVariableNames: ''!!Mixta class methodsFor: 'initialize' stamp: 'lautaro 5/26/2020 20:45'!new: aUnica	^self new new: aUnica; yourself.! !Topografia subclass: #Tierra	instanceVariableNames: ''	classVariableNames: ''	package: 'Tp4-ej3'!!Tierra methodsFor: 'public' stamp: 'lautaro 5/27/2020 19:26'!proporcion	^0.! !!Tierra methodsFor: 'public' stamp: 'lautaro 5/27/2020 18:53'!comprobarIgualdad: top2	^(self calcularProporcion = top2 calcularProporcion).! !!Tierra methodsFor: 'public' stamp: 'lautaro 5/27/2020 19:26'!calcularProporcion	^self proporcion.! !